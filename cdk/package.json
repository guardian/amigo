{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "watch": "tsc -w",
    "test": "jest",
    "test-update": "jest -u",
    "test:dev": "jest --watch",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^7.0.0",
    "tslib": "2.6.2",
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.0",
    "aws-cdk": "2.90.0",
    "aws-cdk-lib": "2.90.0",
    "constructs": "10.2.69",
    "@guardian/cdk": "50.10.13",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "source-map-support": "^0.5.16",
    "typescript": "~5.1.6"
  }
}
