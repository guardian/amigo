{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "watch": "tsc -w",
    "test": "jest",
    "test-update": "jest -u",
    "test:dev": "jest --watch",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/eslint-config-typescript": "^12.0.0",
    "tslib": "2.8.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.29",
    "aws-cdk": "2.172.0",
    "aws-cdk-lib": "2.172.0",
    "constructs": "10.4.2",
    "@guardian/cdk": "61.8.1",
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "source-map-support": "^0.5.16",
    "typescript": "~5.8.3"
  }
}
